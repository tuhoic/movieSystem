{"ast":null,"code":"import axios from '@/api/user';\nexport default {\n  data() {\n    return {\n      form: {\n        username: '',\n        password: '',\n        confirmPassword: '',\n        email: ''\n      },\n      errors: {}\n    };\n  },\n  methods: {\n    async submitForm() {\n      try {\n        const response = await axios.post('/login', this.form);\n        console.log(response.data);\n        // 处理注册成功后的逻辑\n      } catch (error) {\n        console.log(error.response.data);\n        this.errors = error.response.data.errors;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","form","username","password","confirmPassword","email","errors","methods","submitForm","response","post","console","log","error"],"sources":["D:\\graduate\\project\\web\\src\\components\\LoginPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-form\">\r\n    <h2>登录</h2>\r\n    <form @submit.prevent=\"submitForm\">\r\n      <div class=\"form-group\">\r\n        <label for=\"username\">用户名:</label>\r\n        <input type=\"text\" id=\"username\" v-model=\"form.username\" :class=\"{ 'is-invalid': errors.username }\">\r\n        <div class=\"invalid-feedback\">{{ errors.username }}</div>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"password\">密码:</label>\r\n        <input type=\"password\" id=\"password\" v-model=\"form.password\" :class=\"{ 'is-invalid': errors.password }\">\r\n        <div class=\"invalid-feedback\">{{ errors.password }}</div>\r\n      </div>\r\n      <button type=\"submit\" class=\"btn btn-primary\">登录</button>\r\n    </form>\r\n    <p class=\"mt-3\">没有账号？<router-link to=\"/register\">注册</router-link></p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from '@/api/user';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      form: {\r\n        username: '',\r\n        password: '',\r\n        confirmPassword: '',\r\n        email: '',\r\n      },\r\n      errors: {},\r\n    };\r\n  },\r\n  methods: {\r\n    async submitForm() {\r\n      try {\r\n        const response = await axios.post('/login', this.form);\r\n        console.log(response.data);\r\n        // 处理注册成功后的逻辑\r\n      } catch (error) {\r\n        console.log(error.response.data);\r\n        this.errors = error.response.data.errors;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/*.login-form {*/\r\n/*  max-width: 500px;*/\r\n/*  margin: 0 auto;*/\r\n/*}*/\r\n.login-form{\r\n  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.5);\r\n  border-radius: 8px;\r\n  width: 400px;\r\n  max-width: 100%;\r\n  padding: 0px 35px 0;\r\n  position:absolute;\r\n  top:30%;\r\n  left:50%;\r\n  margin:-160px 0 0 -200px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 20px;\r\n}\r\n\r\nlabel {\r\n  font-weight: bold;\r\n}\r\n\r\ninput[type=\"text\"],\r\ninput[type=\"email\"],\r\ninput[type=\"password\"] {\r\n  width: 100%;\r\n  height: 50px;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\nbutton[type=\"submit\"] {\r\n  width: 100%;\r\n  height: 50px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton[type=\"submit\"]:hover {\r\n  background-color: #0069d9;\r\n}\r\n\r\n.invalid-feedback {\r\n  color: red;\r\n  margin-top: 5px;\r\n}\r\n\r\n.is-invalid {\r\n  border-color: red;\r\n}\r\n</style>\r\n"],"mappings":"AAqBA,OAAOA,KAAI,MAAO,YAAY;AAE9B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,eAAe,EAAE,EAAE;QACnBC,KAAK,EAAE;MACT,CAAC;MACDC,MAAM,EAAE,CAAC;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,IAAI,CAAC,QAAQ,EAAE,IAAI,CAACT,IAAI,CAAC;QACtDU,OAAO,CAACC,GAAG,CAACH,QAAQ,CAACT,IAAI,CAAC;QAC1B;MACF,EAAE,OAAOa,KAAK,EAAE;QACdF,OAAO,CAACC,GAAG,CAACC,KAAK,CAACJ,QAAQ,CAACT,IAAI,CAAC;QAChC,IAAI,CAACM,MAAK,GAAIO,KAAK,CAACJ,QA
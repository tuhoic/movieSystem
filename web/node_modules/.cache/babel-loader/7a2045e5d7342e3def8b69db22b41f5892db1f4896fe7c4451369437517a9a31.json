 });\r\n    const countDown = 0;\r\n\r\n    const handleSubmit = async () => {\r\n      try {\r\n        await login(form.username, form.password);\r\n        await router.push('/');\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    };\r\n\r\n    const handleEmailLogin = async () => {\r\n      try {\r\n        await loginWithEmail(form.email, form.emailCode);\r\n        router.push('/');\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    };\r\n\r\n    return {\r\n      ...toRefs(form),\r\n      handleSubmit,\r\n      handleEmailLogin,\r\n      sendEmailCode,\r\n      countDown,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.login-container {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 40px 20px;\r\n}\r\n\r\n.login-title {\r\n  font-size: 24px;\r\n  text-align: center;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.login-form {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n</style>"],"mappings":";AAqEA,SAASA,QAAQ,EAAEC,MAAK,QAAS,KAAK;AACtC,SAASC,SAAQ,QAAS,YAAY;AACtC,SAASC,OAAM,QAAS,uBAAuB;AAE/C,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIJ,SAAS,EAAE;IAC1B,MAAM;MAAEK,KAAK;MAAEC,cAAc;MAAEC;IAAc,IAAIN,OAAO,EAAE;IAC1D,MAAMO,IAAG,GAAIV,QAAQ,CAAC;MACpBW,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,SAAS,EAAE;IACb,CAAC,CAAC;IACF,MAAMC,SAAQ,GAAI,CAAC;IAEnB,MAAMC,YAAW,GAAI,MAAAA,CAAA,KAAY;MAC/B,IAAI;QACF,MAAMT,KAAK,CAACG,IAAI,CAACC,QAAQ,EAAED,IAAI,CAACE,QAAQ,CAAC;QACzC,MAAMN,MAAM,CAACW,IAAI,CAAC,GAAG,CAAC;MACxB,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB;IACF,CAAC;IAED,MAAMG,gBAAe,GAAI,MAAAA,CAAA,KAAY;MACnC,IAAI;QACF,MAAMb,cAAc,CAACE,IAAI,CAACG,KAAK,EAAEH,IAAI,CAACI,SAAS,CAAC;QAChDR,MAAM,CAACW,IAAI,CAAC,GAAG,CAAC;MAClB,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB;IACF,CAAC;IAED,OAAO;MACL,GAAGjB,MAAM,CAACS,IAAI,CAAC;MACfM,YAAY;MACZK,gBAAgB;MAChBZ,aAAa;MA